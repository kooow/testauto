@{
    ViewBag.Title = "Autókereskedés - ASP.NET MVC próbafeladat";
}

<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title>@ViewBag.Title </title>
        <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
        <meta name="viewport" content="width=device-width" />

       @Styles.Render("~/Content/themes/base/css")
       @Styles.Render("~/Content/css")
       @Scripts.Render("~/bundles/modernizr")
       @Scripts.Render("~/bundles/jquery")

       @Scripts.Render("~/bundles/jqueryval")
       @Scripts.Render("~/bundles/jquerydatepicker")

    </head>
    <body>
        <header>
            <div class="content-wrapper">
                <div class="float-left">
                    <p class="site-title">@Html.ActionLink("Autókereskedés", "Index", "Home")</p>
                </div>
                <div class="float-right">
                    <nav>
                        <ul id="menu">
                            <li>@Html.ActionLink("Telephelyek", "List", "Site")</li>
                            <li>@Html.ActionLink("Autók", "List", "Car")</li>
                        </ul>
                    </nav>
                </div>
            </div>
        </header>
        <div id="body">
            @RenderSection("featured", required: false)
            <section class="content-wrapper main-content clear-fix">
                @RenderBody()
            </section>
        </div>
        <footer>

            <div class="content-wrapper">
                <div class="float-left">
                    <p>&copy; @DateTime.Now.Year - @ViewBag.Title</p>
                </div>
            </div>
        </footer>

        @RenderSection("scripts", required: false)

    </body>
</html>

